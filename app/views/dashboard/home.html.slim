            button.btn.btn-success.ml-10 type="submit"  Submit
      /! /.panel-body
      /! / End tabs content
      /! Start pager
      .panel-footer
        ul.pager.wizard.no-margin
          li.previous.disabled
            a href="javascript:void(0);"  Previous
          li.next
            a href="javascript:void(0);"  Next
      /! /.panel-footer
      /! / End pager
    /! /.panel
  /! /#basic-wizard-horizontal
  /! / End basic wizard horizontal


















